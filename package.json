{
  "name": "@BBTechnologies/katiev0test",
  "version": "1.0.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "description": "Collection of UI resources for development",
  "main": "index.js",
  "files": [
    "src",
    "styles",
    "icons",
    "dist"
  ],
  "version": "0.2.0",
  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "dev": "pnpm sass:build && pnpm storybook",
    "build": "pnpm sass:build",
    "precommit:autogenFiles": "git add src/autoGenerated/",
    "precommit": "pnpm sass:build && pnpm test && pnpm audit && pnpm eslint && pnpm precommit:autogenFiles",
    "sass:build": "run-func sass.utilities.js buildSassFiles",
    "sass:watch": "nodemon --watch src/styles --ext scss --exec 'node sass.utilities.js'",
    "eslint": "eslint",
    "eslint:fix": "eslint --fix",
    "test": "jest --config=config/jest/jest.config.js --silent",
    "storybook": "./start-storybook.sh",
    "storybook:dev": "ttab pnpm sass:watch && ttab pnpm storybook",
    "storybook:debug": "pnpm sass:watch && storybook dev -p 9006 -c config/storybook/web --debug-webpack",
    "storybook:build": "pnpm install --frozen-lockfile && pnpm sass:build && storybook build -c config/storybook/web build-storybook -o ./storybook-static"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "html-react-parser": "^5.1.16",
    "next": "^15.0.3",
    "react": "^18",
    "react-dom": "^18",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.53.0",
    "run-func": "^3.0.0",
    "sass": "^1.79.4",
    "storybook": "^8.3.5",
    "use-debounce": "^10.0.3"
  },
