    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^8.2.8",
    "@storybook/react-native": "^7.6.20",
    "@storybook/react-webpack5": "^8.2.8",
    "@storybook/types": "^8.2.8",
    "@testing-library/jest-dom": "^6.6.3",
